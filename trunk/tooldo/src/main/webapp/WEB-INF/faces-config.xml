  <navigation-case>
   <from-outcome>editUser</from-outcome>
   <to-view-id>/pages/secure/admin/user/form.jsf</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>resetPassword</from-outcome>
   <to-view-id>/pages/secure/resetPassword.jsf</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>workflow-main-page</from-outcome>
   <to-view-id>/pages/secure/main.jsf</to-view-id>
  </navigation-case>
 </navigation-rule>
 
 <navigation-rule>
