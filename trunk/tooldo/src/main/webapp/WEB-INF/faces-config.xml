<?xml version="1.0" encoding="ISO-8859-1"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
 <converter>
  <converter-id>ListShuttleRoleConverter</converter-id>
  <converter-class>net.escritoriodigital.unicamp.redefor.converter.ListShuttleRoleConverter</converter-class>
 </converter>
 <converter>
  <converter-id>AlternativeQuestionConverter</converter-id>
  <converter-class>net.escritoriodigital.unicamp.redefor.converter.AlternativeQuestionConverter</converter-class>
 </converter>
 
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>public-content-page</from-outcome>
   <to-view-id>/pages/public/main.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 
 <navigation-rule>
  <from-view-id>/pages/secure/*</from-view-id>
  <navigation-case>
   <from-outcome>preview-content-page</from-outcome>
   <to-view-id>/pages/secure/manager/content/preview.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/secure/main.jsf</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>disciplineSuccess</from-outcome>
   <to-view-id>/pages/secure/admin/discipline/main.jsf</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>courseSuccess</from-outcome>
   <to-view-id>/pages/secure/admin/course/main.jsf</to-view-id>
